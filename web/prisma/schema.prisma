generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Connection {
  id              String   @id @default(uuid())
  name            String
  players         String[]
  connectionId    String?  @default(uuid())
  completed       Int?     @default(0)
  firstConnection Boolean  @default(true)
}
